# Central .env file for all agents

# GitHub Configuration
# GITHUB_TOKEN should be set in your local environment and not committed to the repository
GITHUB_TOKEN=your_github_token_here

# Git Author Configuration
GIT_AUTHOR_NAME=GANON
GIT_AUTHOR_EMAIL=your.email@example.com

# Global Configuration
APPLICATION_SANDBOX_PATH=application-sandbox
DEFAULT_REPO_URL=https://github.com/cr-nattress/ai-devops-lab.git

# Logging Configuration
LOG_LEVEL=INFO
