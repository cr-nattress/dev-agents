# Git Sandbox Agent specific .env file

# Agent Configuration
AGENT_NAME=git-sandbox-agent

# Specific overrides for this agent
# Uncomment and modify these lines to override central values
# GITHUB_TOKEN=agent_specific_token
# GIT_AUTHOR_NAME=Sandbox Agent
# GIT_AUTHOR_EMAIL=sandbox.agent@example.com

# Agent-specific settings
SANDBOX_CLEANUP_TIMEOUT=5  # Timeout in seconds for sandbox cleanup
SANDBOX_PREFIX=sandbox-  # Prefix for sandbox directory names
