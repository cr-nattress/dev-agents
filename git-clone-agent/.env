# Git Clone Agent specific .env file

# Agent Configuration
AGENT_NAME=git-clone-agent

# Specific overrides for this agent
# Uncomment and modify these lines to override central values
# GITHUB_TOKEN=agent_specific_token
# GIT_AUTHOR_NAME=Clone Agent
# GIT_AUTHOR_EMAIL=clone.agent@example.com

# Agent-specific settings
CLONE_DEPTH=1  # Shallow clone by default
CLONE_TIMEOUT=60  # Timeout in seconds
