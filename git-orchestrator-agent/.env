# Git Orchestrator Agent specific .env file

# Agent Configuration
AGENT_NAME=git-orchestrator-agent

# Specific overrides for this agent
# Uncomment and modify these lines to override central values
# GITHUB_TOKEN=agent_specific_token
# GIT_AUTHOR_NAME=Orchestrator Agent
# GIT_AUTHOR_EMAIL=orchestrator.agent@example.com

# Agent-specific settings
WORKFLOW_TIMEOUT=300  # Timeout in seconds for the entire workflow
DEFAULT_BASE_BRANCH=main  # Default base branch for PRs
DEFAULT_PR_TITLE=Automated PR by Git Orchestrator Agent
